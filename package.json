{
    "name": "word-search",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "concurrently --kill-others \"npm run start:go\" \"ng serve\"",
        "start:go": "nodemon -e go -x \"npm run build:go\"",
        "build": "npm run build:go && ng build",
        "build:angular": "ng build --configuration production",
        "build:go": "env GOOS=js GOARCH=wasm go build -o ./src/assets/main.wasm"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^19.2.0",
        "@angular/compiler": "^19.2.0",
        "@angular/core": "^19.2.0",
        "@angular/forms": "^19.2.0",
        "@angular/platform-browser": "^19.2.0",
        "@angular/platform-browser-dynamic": "^19.2.0",
        "@angular/router": "^19.2.0",
        "@pdf-lib/fontkit": "^1.1.1",
        "@tailwindcss/postcss": "^4.1.4",
        "canvas-confetti": "^1.9.3",
        "pdf-lib": "^1.17.1",
        "postcss": "^8.5.3",
        "qrcode": "^1.5.4",
        "rxjs": "~7.8.0",
        "tailwindcss": "^4.1.4",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.2.5",
        "@angular/cli": "^19.2.5",
        "@angular/compiler-cli": "^19.2.0",
        "@types/canvas-confetti": "^1.9.0",
        "@types/qrcode": "^1.5.5",
        "concurrently": "^9.1.2",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.3",
        "typescript": "~5.7.2"
    }
}
